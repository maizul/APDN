
@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<table class="table table-bordered">
    <tr>
        <th>Name: </th>
        <th>Id: </th>
        <th>Price: </th>
    </tr>
    @foreach(var p in Model)
            {
                <tr>
                    <td>@p.Name</td>
                    <td>@p.Id</td>
                    <td>@p.Price</td>
                </tr>
            }
</table>

