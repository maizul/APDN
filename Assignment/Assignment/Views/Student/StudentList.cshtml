@model List<Assignment.Models.DB.Student>
@{
    ViewBag.Title = "StudentList";
}

<h2>StudentList</h2>

<table class="table table-bordered">
    <tr>
        <th> ID</th>
        <th> Name</th>
        <th></th>
        <th></th>
    </tr>
    @foreach (var p in Model)
    {
<tr>
    <td> @p.Id</td>
    <td>@p.Name</td>

    <td><a href="/Student/Edit/@p.Id">Edit</a></td>
    <td><a href="/Student/Delete/@p.Id">Edit</a></td>

</tr>


    }


</table>